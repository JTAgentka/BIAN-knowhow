{
    "definitions": {
        "ORDERER": {
            "description": "Orderrer",
            "type": "object",
            "properties": {
                "ID": {
                    "description": "ID",
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 0,
                    "maxLength": 10
                },
                "NAME": {
                    "description": "Name",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "ADDRESS": {
                    "description": "Address",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "ID_NUM": {
                    "description": "ID_NUM",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "ID_CARD": {
                    "description": "ID_CARD",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            }
        },
        "XRATE": {
            "type": [
                "array",
                "null"
            ],
            "title": "XRATE",
            "description": "Used xrates",
            "items": {
                "type": "object",
                "properties": {
                    "CURRENCY": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "minLength": 3,
                        "maxLength": 3,
                        "title": "CURRENCY",
                        "description": "exchange rate currency"
                    },
                    "EXCHANGE_RATE": {
                        "type": "number",
                        "title": "EXCHANGE_RATE",
                        "description": "exchange rate"
                    },
                    "QUOTATION_UNIT": {
                        "type": [
                            "number",
                            "null"
                        ],
                        "title": "QUOTATION_UNIT",
                        "description": "quotation unit"
                    }
                }
            },
            "required": [
                "EXCHANGE_RATE"
            ]
        }
    },
    "type": "object",
    "properties": {
        "X-Correlation-ID": {
            "type": [
                "string",
                "null"
            ],
            "title": "X-Correlation-ID",
            "description": "Correlation identifier"
        },
        "BOOK_ID": {
            "description": "BISQ booking internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "EVENT_TIMESTAMP": {
            "description": "Date and time of inserting into event table in BISQ",
            "type": "string",
            "format": "date-time"
        },
        "ACT_ID": {
            "description": "BISQ activity internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "STATE": {
            "description": "BISQ activity state (A)Authorised, (B)Booked, (S)Reversed",
            "type": "string",
            "enum": [
                "A",
                "B",
                "S"
            ]
        },
        "IND_DC": {
            "description": "Indicator (C)Credit, (D)Debit",
            "type": "string",
            "enum": [
                "C",
                "D"
            ]
        },
        "ACT_TYPE": {
            "description": "BISQ activity type code\nTLM31000 - Výběr hotovosti CZK/CZK\nTLM31100 - Výběr hotovosti CM/CM\nTLM31200 - Výběr hotovosti CM1/CM2\nTLM31300 - Výběr hotovosti CM/CZK\nTLM31400 - Výběr hotovosti CZK/CM\nTLM32000 - Vklad hotovosti CZK/CZK\nTLM32100 - Vklad hotovosti CM/CM\nTLM32200 - Vklad hotovosti CM1/CM2\nTLM32300 - Vklad hotovosti CM/CZK\nTLM32400 - Vklad hotovosti CZK/CM",
            "type": "string",
            "minLength": 8,
            "maxLength": 8
        },
        "STM_TYPE": {
            "description": "Statement type code",
            "type": "string",
            "minLength": 0,
            "maxLength": 20
        },
        "PC_ID": {
            "description": "BISQ product contract internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "PR_NUMBER": {
            "description": "BISQ product number",
            "type": "string",
            "minLength": 1,
            "maxLength": 35
        },
        "BOOK_DATE": {
            "description": "Booing date",
            "type": "string",
            "format": "date"
        },
        "VALUE_DATE": {
            "description": "Value date",
            "type": "string",
            "format": "date"
        },
        "EFFECTIVE_DATE": {
            "description": "Effective date",
            "type": "string",
            "format": "date"
        },
        "ACT_SEC_NUM": {
            "description": "Daily sequence number",
            "type": "integer"
        },
        "ACC_AMOUNT": {
            "description": "Booking amount",
            "type": "number"
        },
        "ACC_CURRENCY": {
            "description": "Booking currency",
            "type": "string",
            "minLength": 3,
            "maxLength": 3
        },
        "TRN_AMOUNT": {
            "description": "Transaction amount",
            "type": "number"
        },
        "TRN_CURRENCY": {
            "description": "Transaction currency",
            "type": "string",
            "minLength": 3,
            "maxLength": 3
        },
        "XRATE": {
            "$ref": "#/definitions/XRATE"
        },
        "XRATE_DEVIATION": {
            "description": "exchange rate deviation against the ECB",
            "type": [
                "number",
                "null"
            ]
        },
        "ORDERER": {
            "$ref": "#/definitions/ORDERER"
        },
        "CASHIER": {
            "description": "BISQ teller window description",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 35
        },
        "DESCRIPTION": {
            "description": "O2R message",
            "type": [
                "string",
                "null"
            ],
            "minLength": 0,
            "maxLength": 210
        },
        "PRODUCT_KIND": {
            "description": "Product kind",
            "type": "string",
            "enum": [
                "CUA",
                "CCR",
                "LOA",
                "RDA",
                "NWB",
                "CRD"
            ]
        }
    },
    "required": [
        "BOOK_ID",
        "EVENT_TIMESTAMP",
        "ACT_ID",
        "STATE",
        "IND_DC",
        "ACT_TYPE",
        "STM_TYPE",
        "PC_ID",
        "PR_NUMBER",
        "BOOK_DATE",
        "VALUE_DATE",
        "EFFECTIVE_DATE",
        "ACT_SEC_NUM",
        "ACC_AMOUNT",
        "ACC_CURRENCY",
        "TRN_AMOUNT",
        "TRN_CURRENCY",
        "PRODUCT_KIND"
    ]
}