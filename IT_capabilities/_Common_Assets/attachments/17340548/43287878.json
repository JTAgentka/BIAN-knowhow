{
  "title": "mutualFunds",
  "description": "Mutual Funds",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "confirmationId",
    "owner",
    "confirmationTypeId",
    "documentLanguage",
    "generatedDate",
    "insertedDate",
    "businessChangeDate",
    "stateId",
    "mutualFunds"
  ],
  "properties": {
    "X-Correlation-ID": {
      "description": "Korelační identifikátor, povinné pro INIT a RECON",
      "type": [
        "string",
        "null"
      ]
    },
    "confirmationId": {
      "type": "integer",
      "description": "Id dokumentu"
    },
    "owner": {
      "type": "object",
      "description": "Detaily vlastníka",
      "$ref": "#/definitions/person"
    },
    "portfolioIds": {
      "type": "object",
      "description": "Identifikátory portfolia",
      "$ref": "#/definitions/portfolioIds"
    },
    "confirmationTypeId": {
      "description": "Hodnoty odkazující na typ konfirmace(bis10, ocp01,ocp03, ocp04, ocp06, ocp08, ocp10, ocp12, ocp13, ocp14, ocp24, ocp37, ocp39, ocp40, ocp41, ocp43, ocp44, ocp45, ocp46)",
      "type": "string"
    },
    "documentLanguage": {
      "description": "Jazyk dokumentu - CZE, ENG, SLO",
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    },
    "generatedDate": {
      "description": "čas vygenerování dokumentu",
      "type": "string",
      "format": "date-time"
    },
    "insertedDate": {
      "description": "čas vložení záznamu do db",
      "type": "string",
      "format": "date-time"
    },
    "businessChangeDate": {
      "description": "Poslední změna statusu",
      "type": "string",
      "format": "date-time"
    },
    "stateId": {
      "description": "status zpracování - status=9(posláno do CRM)  nebo 10 (chyba), ostatní nejsou relevantní",
      "type": "integer"
    },
    "files": {
      "description": "Soubory které jsou součástí výpisu",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "object",
        "required": [
          "contentType",
          "fileName"
        ],
        "properties": {
          "contentType": {
            "description": "Typ obsahu",
            "type": "string",
            "minLength": 0,
            "maxLength": 20
          },
          "fileName": {
            "description": "Jméno souboru",
            "type": "string",
            "minLength": 0,
            "maxLength": 100
          }
        }
      }
    },
    "mutualFunds": {
      "type": "object",
      "required": [
        "fee",
        "securityName",
        "isin",
        "orderType",
        "orderIds",
        "transactionCurrency",
        "managingCompanyTag"
      ],
      "properties": {
        "directionTransaction": {
          "description": "S-subscription/nákup,R-redemption-prodej   --- prodej, odkup --nákup investice",
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "S",
            "R",
            null
          ]
        },
        "directionTransactionLabel": {
          "description": "nákup, prodej do jazyku klienta",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 30
        },
        "counterParty": {
          "description": "označení protistrany dle MIFID v jazyku klienta, vždy protistrana nebo counterparty, nebo obchodník/broker",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 30
        },
        "orderIds": {
          "type": "object",
          "description": "Identifikátory pokynu",
          "$ref": "#/definitions/orderIds"
        },
        "orderDate": {
          "description": "datum objednávky",
          "type": [
            "string",
            "null"
          ],
          "format": "date"
        },
        "transactionDate": {
          "description": "datum obchodu",
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "settlementDate": {
          "description": "Datum vypořádání",
          "type": [
            "string",
            "null"
          ],
          "format": "date"
        },
        "orderPlaceRealization": {
          "description": "Distributor fondu",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 250
        },
        "securityName": {
          "description": "Název cenného papíru",
          "type": "string",
          "minLength": 1,
          "maxLength": 250
        },
        "isin": {
          "description": "ISIN",
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        },
        "orderType": {
          "description": "klientský překlad typu operace v ta(arbesta.ut_operationtype)",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 250
        },
        "quantity": {
          "description": "počet kusů cenného papíru",
          "type": [
            "number",
            "null"
          ]
        },
        "instrumentPrice": {
          "description": "cena za jeden kus cenného papíru",
          "type": [
            "number",
            "null"
          ]
        },
        "transactionValue": {
          "description": "hodnota transakce/obchodu v měně transakce bez poplatku",
          "type": [
            "number",
            "null"
          ]
        },
        "transactionCurrency": {
          "description": "měna transakce",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "fxRate": {
          "description": "kurz transakce",
          "type": [
            "number",
            "null"
          ]
        },
        "fee": {
          "description": "poplatek",
          "type": [
            "number"
          ]
        },
        "transactionValueTotal": {
          "description": "hodnota operace/pokynu v měně operace včetně poplatku",
          "type": [
            "number",
            "null"
          ]
        },
        "transactionCurrencyTotal": {
          "description": "celková měna operace",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "feeCurrency": {
          "description": "měna poplatku",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "newOrChangeLabel": {
          "description": "popis zda se jedná o změnovou konfirmaci v jazyce klienta",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 30
        },
        "orderCurrency": {
          "description": "měna pokynu",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "managingCompanyTag": {
          "description": "Příslušnost portfolia ke společnosti - barva klienta. silver - J&T ČR, green - J&T SR",
          "type": "string",
          "enum": [
            "SILVER",
            "GREEN"
          ]
        },
        "newOrChange": {
          "description": "NEW/CHANGE - jestli mění stávající konfirmaci",
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "NEW",
            "CHANGE",
            null
          ]
        },
        "portfolioIds": {
          "type": "object",
          "description": "Identifikátory portfolia",
          "$ref": "#/definitions/portfolioIds"
        },
        "feeDiscount": {
          "description": "sleva na poplatku (z magnus bodů)",
          "type": [
            "number",
            "null"
          ]
        },
        "feeDiscountCurrency": {
          "description": "měna slevy na poplatku",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "feeAfterDiscount": {
          "description": "poplatek se započtenou slevou (z magnus bodů)",
          "type": [
            "number",
            "null"
          ]
        },
        "feeDiscountAfterCurrency": {
          "description": "poplatek se započtenou slevou (z magnus bodů) měna",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "taxFee": {
          "description": "srážková daň",
          "type": [
            "number",
            "null"
          ]
        },
        "taxFeeCurrency": {
          "description": "měna srážkové daně",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "fxRateCurrencyPair": {
          "description": "kurz měnového páru",
          "type": [
            "number",
            "null"
          ]
        },
        "feePercentage": {
          "description": "procentuální vyjádření poplatku k celé transakci",
          "type": [
            "number",
            "null"
          ]
        },
        "feePercentageUnit": {
          "description": "měna procentuálního vyjádření poplatku k transakci",
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 1
        },
        "feeTotal": {
          "description": "celkové poplatky",
          "type": [
            "number",
            "null"
          ]
        },
        "feeTotalCurrency": {
          "description": "Měna celkových poplatků",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        },
        "roundingDifference": {
          "description": "zaokrouhlovací rozdíl",
          "type": [
            "number",
            "null"
          ]
        },
        "roundingDifferenceCurrency": {
          "description": "měna zaokrouhlovacího rozdílu",
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        }
      }
    }
  },
  "definitions": {
    "portfolioIds": {
      "description": "Identifikátory portfolia",
      "type": "object",
      "properties": {
        "topasId": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Identifikace Portfolia v TOPAS (KID)"
        },
        "taId": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Identifikace Účtu podílníka v TA"
        }
      }
    },
    "person": {
      "description": "Detaily osoby",
      "type": "object",
      "required": [
        "crmId"
      ],
      "properties": {
        "crmId": {
          "type": "string",
          "description": "Unikátní CRM identifikátor",
          "minLength": 10,
          "maxLength": 10
        }
      }
    },
    "orderIds": {
      "description": "Identifikátory pokynů",
      "type": "object",
      "properties": {
        "taId": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Identifikace Účtu podílníka v TA"
        }
      }
    }
  }
}