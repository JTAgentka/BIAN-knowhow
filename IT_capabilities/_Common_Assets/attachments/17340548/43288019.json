{
  "title": "Transaction notification",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Transaction notification",
  "type": "object",
  "required": [
    "requestId",
    "requestDate",
    "terminalDate",
    "authorizationDate",
    "terminalId",
    "source",
    "aPan",
    "pPan",
    "cardFiid",
    "authorizationType",
    "transactionCode",
    "authorizationAmount",
    "authorizationCurrency",
    "effectiveAmount",
    "effectiveCurrency"
  ],
  "properties": {
    "X-Correlation-ID": {
      "type": [
        "string",
        "null"
      ],
      "title": "X-Correlation-ID",
      "description": "Correlation identifier"
    },
    "requestId": {
      "type": "string",
      "title": "requestId",
      "minLength": 0,
      "maxLength": 19,
      "description": "request Id"
    },
    "requestDate": {
      "type": "string",
      "title": "requestDate",
      "description": "request Date",
      "format": "date-time"
    },
    "terminalDate": {
      "type": "string",
      "title": "terminalDate",
      "description": "terminal Date",
      "format": "date-time"
    },
    "authorizationDate": {
      "type": "string",
      "title": "authorizationDate",
      "description": "authorization Date",
      "format": "date-time"
    },
    "terminalId": {
      "type": "string",
      "title": "terminalId",
      "minLength": 0,
      "maxLength": 16,
      "description": "terminal Id"
    },
    "source": {
      "description": "source",
      "type": "string",
      "title": "source",
      "minLength": 3,
      "maxLength": 3,
      "enum": [
        "POS",
        "ATM"
      ]
    },
    "aPan": {
      "type": "string",
      "title": "pan",
"pattern": "^[0-9]{12,19}$",
      "description": "card pan"
    },
    "pPan": {
      "type": "string",
      "title": "pan",
      "pattern": "^[0-9]{12,19}$",
      "description": "card pan"
    },
    "cardFiid": {
      "type": "string",
      "title": "cardFiid",
        "minLength": 4,
        "maxLength": 4,
      "description": "card Fiid"
    },
    "retailerFiid": {
        "type": [
            "string",
            "null"
        ],
      "title": "retailerFiid",
        "minLength": 4,
        "maxLength": 4,
      "description": "retailer Fiid"
    },
    "authorizationType": {
      "type": "integer",
      "title": "authorizationType",
      "description": "authorization Type"
    },
    "transactionCode": {
      "type": "string",
      "title": "transactionCode",
        "minLength": 0,
        "maxLength": 6,
      "description": "transaction Code"
    },
    "responseCode": {
        "type": [
            "integer",
            "null"
        ],
      "title": "responseCode",
      "description": "response Code"
    },
    "authorizationCode": {
        "type": [
            "string",
            "null"
        ],
      "title": "authorizationCode",
        "minLength": 0,
        "maxLength": 8,
      "description": "authorization Code"
    },
    "ecommerceFlag": {
        "type": [
            "string",
            "null"
        ],
      "title": "ecommerceFlag",
        "minLength": 1,
        "maxLength": 1,
      "description": "ecommerce Flag"
    },
    "entryMode": {
        "type": [
            "integer",
            "null"
        ],
      "title": "entryMode",
      "description": "entry Mode"
    },
    "terminalOwner": {
        "type": [
            "string",
            "null"
        ],
      "title": "terminalOwner",
        "minLength": 0,
        "maxLength": 22,
      "description": "terminal Owner"
    },
    "terminalLocation": {
        "type": [
            "string",
            "null"
        ],
      "title": "terminalLocation",
        "minLength": 0,
        "maxLength": 25,
      "description": "terminal Location"
    },
    "terminalCity": {
        "type": [
            "string",
            "null"
        ],
      "title": "terminalCity",
        "minLength": 0,
        "maxLength": 13,
      "description": "terminal City"
    },
    "terminalCountry": {
        "type": [
            "string",
            "null"
        ],
      "title": "terminalCountry",
        "minLength": 2,
        "maxLength": 2,
      "description": "terminal Country"
    },
    "authorizationAmount": {
      "type": "number",
      "title": "authorizationAmount",
      "description": "authorization Amount"
    },
    "authorizationAmount2": {
        "type": [
            "number",
            "null"
        ],
      "title": "authorizationAmount2",
      "description": "authorization Amount 2"
    },
    "originalAmount": {
        "type": [
            "number",
            "null"
        ],
      "title": "originalAmount",
      "description": "original Amount"
    },
    "authorizationCurrency": {
      "type": "integer",
      "title": "authorizationCurrency",
      "description": "authorization Currency"
    },
    "originalCurrency": {
        "type": [
            "integer",
            "null"
        ],
      "title": "originalCurrency",
      "description": "original Currency"
    },
    "effectiveAmount": {
      "type": "number",
      "title": "effectiveAmount",
      "description": "effective Amount"
    },
    "effectiveCurrency": {
      "type": "integer",
      "title": "effectiveCurrency",
      "description": "effectiveCurrency"
    },
    "cafData": {
        "type": [
            "string",
            "null"
        ],
      "title": "cafData",
        "minLength": 0,
        "maxLength": 200,
      "description": "card Data"
    }
  }
}