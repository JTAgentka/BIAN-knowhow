{
  "title": "Payment",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Payment",
  "type": "object",
  "required": [
    "ptsId",
    "businessChangeDate",
    "channel",
    "type",
    "businessState",
    "requestedExecutionDate",
    "instructedAmount",
    "debtor",
    "creditor"
  ],
  "properties": {
    "X-Correlation-ID": {
      "type": [
        "string",
        "null"
      ],
      "title": "X-Correlation-ID",
      "description": "Correlation identifier"
    },
    "ptsId": {
      "type": "integer",
      "title": "ptsId",
      "description": "id platby PTS"
    },
    "businessChangeDate": {
      "type": "string",
      "format": "date-time",
      "title": "businessChangeDate",
      "description": "datum business změny stavu datové entity"
    },
    "identification": {
      "type": "object",
      "title": "identification",
      "description": "identification",
      "properties": {
        "instructionIdentification": {
          "type": "string",
          "title": "instructionIdentification",
          "description": "ID platby zaslané do PTS"
        },
        "operationId": {
          "type": "string",
          "title": "operationId",
          "description": "operation ID zasílané z externího systému"
        },
        "clientId": {
          "type": [
            "string",
            "null"
          ],
          "title": "clientId",
          "description": "identifikace klienta (KLID), který platbu inicioval"
        }
      }
    },
    "channel": {
      "type": "string",
      "title": "channel",
      "description": "komunikační kanál"
    },
    "chargeBearer": {
      "type": [
        "string",
        "null"
      ],
      "title": "chargeBearer",
      "description": "kód poplatku",
      "enum": [
        "CRED",
        "DEBT",
        "SHAR",
        "SLEV"
      ]
    },
    "instructionPriority": {
      "type": [
        "string",
        "null"
      ],
      "title": "instructionPriority",
      "description": "priorita platby",
      "enum": [
        "HIGH",
        "INST",
        "NORM"
      ]
    },
    "type": {
      "type": "string",
      "title": "type",
      "description": "typ platby",
      "enum": [
        "CZ",
        "FIN",
        "SEPA"
