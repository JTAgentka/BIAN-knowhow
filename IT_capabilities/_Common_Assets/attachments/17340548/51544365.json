{
  "title": "Codebook",
  "description": "Codebook schema",
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "properties": {
    "codebook": {
      "type": "object",
      "properties": {
        "codebookId": {
          "type": "string",
          "description": "Unique identifier of the codebook"
        },
        "businessChangeDate": {
          "type": "string",
          "format": "date-time",
          "description": "Date and time of publication of the change"
        },
        "codebookItems": {
          "type": [
            "array",
            "null"
          ],
          "description": "Array of codebook items",
          "required": [
            "codebookItemId"
          ],
          "items": {
            "type": "object",
            "properties": {
              "codebookItemId": {
                "type": "string",
                "description": "Unique identifier of the codebook item used in Kafka"
              },
              "codebookApiItemId": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Unique identifier of the codebook item used in API"
              },
              "codebookItemNameCz": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Codebook item name in Czech language"
              },
              "codebookItemNameEnCz": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Translation of the codebook item name from Czech into English"
              },
              "codebookItemNameSk": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Codebook item name in Slovak language"
              },
              "codebookItemNameEnSk": {
                "type": [
                  "string",
                  "null"
                ],
                "description": "Translation of the codebook item name from Slovak into English"
              },
              "position": {
                "type": [
                  "integer",
                  "null"
                ],
                "description": "An integer specifying the order of the item"
              },
              "validityDateStart": {
                "type": [
                  "string",
                  "null"
                ],
                "format": "date-time",
                "description": "Date and time of the item validity start"
              },
              "validityDateEnd": {
                "type": [
                  "string",
                  "null"
                ],
                "format": "date-time",
                "description": "Date and time of the item validity end"
              },
              "additionalData": {
                "type": "object",
                "description": "OneOf",
                "properties": {
                  "accountGroup": {
                    "$ref": "#/definitions/accountGroup"
                  },
                  "accountMovementType": {
                    "$ref": "#/definitions/accountMovementType"
                  },
                  "accountType": {
                    "$ref": "#/definitions/accountType"
                  },
                  "applicationType": {
                    "$ref": "#/definitions/applicationType"
                  },
                  "bicBank": {
                    "$ref": "#/definitions/bicBank"
                  },
                  "cardGroup": {
                    "$ref": "#/definitions/cardGroup"
                  },
                  "cid": {
                    "$ref": "#/definitions/cid"
                  },
                  "contractType": {
                    "$ref": "#/definitions/contractType"
                  },
                  "country": {
                    "$ref": "#/definitions/country"
                  },
                  "currency": {
                    "$ref": "#/definitions/currency"
                  },
                  "czechBank": {
                    "$ref": "#/definitions/czechBank"
                  },
                  "depositoryAccType": {
                    "$ref": "#/definitions/depositoryAccType"
                  },
                  "enterpriseSystemList": {
                    "$ref": "#/definitions/enterpriseSystemList"
                  },
                  "fxRateList": {
                    "$ref": "#/definitions/fxRateList"
                  },
                  "invExchange": {
                    "$ref": "#/definitions/invExchange"
                  },
                  "invExchangeExceptionOpening": {
                    "$ref": "#/definitions/invExchangeExceptionOpening"
                  },
                  "invExchangeStandardOpening": {
                    "$ref": "#/definitions/invExchangeStandardOpening"
                  },
                  "job": {
                    "$ref": "#/definitions/job"
                  },
                  "language": {
                    "$ref": "#/definitions/language"
                  },
                  "legalCategoryType": {
                    "$ref": "#/definitions/legalCategoryType"
                  },
                  "partyStatus": {
                    "$ref": "#/definitions/partyStatus"
                  },
                  "paymentPeriod": {
                    "$ref": "#/definitions/paymentPeriod"
                  },
                  "productType": {
                    "$ref": "#/definitions/productType"
                  },
                  "region": {
                    "$ref": "#/definitions/region"
                  },
                  "regularInvestmentFrequency": {
                    "$ref": "#/definitions/regularInvestmentFrequency"
                  },
                  "realtimeDataForm": {
                    "$ref": "#/definitions/realtimeDataForm"
                  },
                  "realtimeDataFormQuestion": {
                    "$ref": "#/definitions/realtimeDataFormQuestion"
                  },
                  "realtimeDataQuestion": {
                    "$ref": "#/definitions/realtimeDataQuestion"
                  },
                  "securitiesSubType": {
                    "$ref": "#/definitions/securitiesSubType"
                  },
                  "securitiesType": {
                    "$ref": "#/definitions/securitiesType"
                  },
                  "segment": {
                    "$ref": "#/definitions/segment"
                  },
                  "standingOrderEvaluation": {
                    "$ref": "#/definitions/standingOrderEvaluation"
                  },
                  "statementDistributionChannel": {
                    "$ref": "#/definitions/statementDistributionChannel"
                  },
                  "statementType": {
                    "$ref": "#/definitions/statementType"
                  },
                  "subsegment": {
                    "$ref": "#/definitions/subsegment"
                  },
                  "targetMarketCustomerCategory": {
                    "$ref": "#/definitions/targetMarketCustomerCategory"
                  },
                  "targetMarketInvestmentTarget": {
                    "$ref": "#/definitions/targetMarketInvestmentTarget"
                  },
                  "timeChange": {
                    "$ref": "#/definitions/timeChange"
                  }
                }
              }
            }
          }
        }
      },
      "required": [
        "codebookId",
        "businessChangeDate"
      ],
      "additionalProperties": true
    }
  },
  "definitions": {
    "accountGroup": {
      "type": "object",
