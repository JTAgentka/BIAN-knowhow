        "presentationTemplateId": {
          "type": [
            "string",
            "null"
          ],
          "description": "Unique identifier of the presentation template"
        },
        "group": {
          "type": [
            "string",
            "null"
          ],
          "description": "Unique identifier of the group"
        }
      },
      "additionalProperties": true
    },
    "accountType": {
      "type": "object",
      "properties": {
        "group": {
          "type": [
            "string",
            "null"
          ],
          "description": "Unique identifier of the group"
        }
      },
      "additionalProperties": true
    },
    "applicationType": {
      "type": "object",
