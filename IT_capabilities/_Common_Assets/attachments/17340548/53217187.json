{
    "definitions": {
        "COUNTERPARTY": {
            "description": "Counterparty",
            "type": "object",
            "properties": {
                "PC_ID": {
                    "description": "Destination Quaestor account internal identification",
                    "type": "string",
                    "minLength": 20,
                    "maxLength": 20
                },
                "ACC_NUM": {
                    "description": "Destination Account Number",
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 35
                },
                "NCSC": {
                    "description": "Destination numeric bank code",
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 4,
                    "maxLength": 4
                },
                "ADDRESS": {
                    "description": "Destination address",
                    "type": [
                        "array",
                        "null"
                    ],
                    "items": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "maxItems": 4
                },
                "COUNTRY": {
                    "description": "Destination country",
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 2,
                    "maxLength": 2
                },
                "BIC": {
                    "description": "Destination BIC",
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 11,
                    "maxLength": 11
                }
            },
            "required": [
                "ACC_NUM"
            ]
        }
    },
    "type": "object",
    "properties": {
        "X-Correlation-ID": {
            "type": [
                "string",
                "null"
            ],
            "title": "X-Correlation-ID",
            "description": "Correlation identifier"
        },
        "BOOK_ID": {
            "description": "BISQ booking internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "EVENT_TIMESTAMP": {
            "description": "Date and time of inserting into event table in BISQ",
            "type": "string",
            "format": "date-time"
        },
        "ACT_ID": {
            "description": "BISQ activity internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "STATE": {
            "description": "BISQ activity state (A)Authorised, (B)Booked, (S)Reversed",
            "type": "string",
            "enum": [
                "A",
                "B",
                "S"
            ]
        },
        "IND_DC": {
            "description": "Indicator (C)Credit, (D)Debit",
            "type": "string",
            "enum": [
                "C",
                "D"
            ]
        },
        "ACT_TYPE": {
            "description": "BISQ activity type code",
            "type": "string",
            "minLength": 0,
            "maxLength": 8
        },
        "STM_TYPE": {
            "description": "Statement type code",
            "type": "string",
            "minLength": 0,
            "maxLength": 20
        },
        "DESCRIPTION": {
            "description": "BISQ activity type description",
            "type": "string",
            "minLength": 0,
            "maxLength": 50
        },
        "PC_ID": {
            "description": "BISQ product contract internal ID",
            "type": "string",
            "minLength": 20,
            "maxLength": 20
        },
        "PR_NUMBER": {
            "description": "BISQ product number",
            "type": "string",
            "minLength": 1,
            "maxLength": 35
        },
        "COUNTERPARTY": {
            "$ref": "#/definitions/COUNTERPARTY"
        },
        "BOOK_DATE": {
            "description": "Booing date",
            "type": "string",
            "format": "date"
        },
        "VALUE_DATE": {
            "description": "Value date",
            "type": "string",
            "format": "date"
        },
        "EFFECTIVE_DATE": {
            "description": "Effective date",
            "type": "string",
            "format": "date"
        },
        "ACC_AMOUNT": {
            "description": "Booking amount",
            "type": "number"
        },
        "ACC_CURRENCY": {
            "description": "Booking currency",
            "type": "string",
            "minLength": 3,
            "maxLength": 3
        },
        "TRN_AMOUNT": {
            "description": "Transaction amount",
            "type": "number"
        },
        "TRN_CURRENCY": {
            "description": "Transaction currency",
            "type": "string",
            "minLength": 3,
            "maxLength": 3
        },
        "REVERSAL": {
            "description": "Reversal indicator",
            "type": [
                "string",
                "null"
            ],
            "default": "N",
            "enum": [
                "Y",
                "N"
            ]
        },
        "PRODUCT_KIND": {
            "description": "Product kind",
            "type": "string",
            "enum": [
                "CUA",
                "CCR",
                "LOA",
                "RDA",
                "NWB",
                "CRD"
            ]
        }
    },
    "required": [
        "BOOK_ID",
        "EVENT_TIMESTAMP",
        "ACT_ID",
        "STATE",
        "IND_DC",
        "ACT_TYPE",
        "STM_TYPE",
        "DESCRIPTION",
        "PC_ID",
        "PR_NUMBER",
        "BOOK_DATE",
        "VALUE_DATE",
        "EFFECTIVE_DATE",
        "ACC_AMOUNT",
        "ACC_CURRENCY",
        "TRN_AMOUNT",
        "TRN_CURRENCY",
        "PRODUCT_KIND"
    ]
}