    "required": [
        "BOOK_ID",
        "EVENT_TIMESTAMP",
        "ACT_ID",
        "STATE",
        "IND_DC",
        "ACT_TYPE",
        "STM_TYPE",
        "TYPE",
        "DESCRIPTION",
        "PC_ID",
        "PR_NUMBER",
        "BOOK_DATE",
        "VALUE_DATE",
        "EFFECTIVE_DATE",
        "ACC_AMOUNT",
        "ACC_CURRENCY",
        "TRN_AMOUNT",
        "TRN_CURRENCY",
        "PRODUCT_KIND"
