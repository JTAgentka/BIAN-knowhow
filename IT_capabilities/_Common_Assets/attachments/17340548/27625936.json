{
  "title": "InstrumentValuePriceInstantDelayed_v2",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Ocenovací ceny CP aktualni zpozdene; Topic bude zasilan se zmenou nektereho z atributu topicu, minimalne jednou za den. Topic bude obsahovat ocenovaci ceny - z vodopadu cen (viz Zavislost DK* vs. MDP).",
  "type": "object",
  "required": [
    "instrumentId",
    "validityDate",
    "businessChangeDate",
    "status",
    "isin"
  ],
  "properties": {
    "X-Correlation-ID": {
      "type": [
        "string",
        "null"
      ],
      "title": "X-Correlation-ID",
      "description": "Correlation identifier"
    },
    "instrumentId": {
      "type": "integer",
      "title": "instrumentId",
      "description": "ID instrumentu / CP"
    },
    "validityDate": {
      "type": "string",
      "format": "date",
      "title": "validityDate",
      "description": "Datum platnosti ceny"
    },
    "businessChangeDate": {
      "type": "string",
      "format": "date-time",
      "title": "businessChangeDate",
      "description": "Datum a cas posledni zmeny entity"
    },
    "status": {
      "type": "string",
      "title": "status",
      "enum": [
        "active",
        "deleted"
      ],
      "description": "Stav businessove platnosti entity active - aktivni, deleted - info o zrusenem zaznamu"
    },
    "isin": {
      "type": "string",
      "title": "isin",
      "description": "DK CZ - pro jedno instrumentID nemuze existovat vice ruznych isin"
    },
    "currency": {
      "type": [
        "string",
        "null"
      ],
      "title": "currency",
      "description": "Mena obchodovani CP - ISO alpha(3)"
    },
    "nominalValue": {
      "type": [
        "number",
        "null"
      ],
      "title": "nominalValue",
      "description": "Ceny pro Amortizované dluhopisy"
    },
    "buyPrice": {
      "description": "oceňovací cena CP typu 'bid' (trh nakupuje)",
      "type": "object",
      "properties": {
        "price": {
          "type": "number",
          "description": "cena v měně currency"
        },
        "marketId": {
          "type": "string",
          "description": "Apl_Kod Instituce trhu, ze kterého pochází hodnoty (číselník)"
        },
        "datafeedId": {
          "type": [
            "string",
            "null"
          ],
          "description": "Apl_Kod Datafeedu, ze kterého pochází hodnoty (číselník)"
        }
      }
    },
    "sellPrice": {
      "description": "oceňovací cena CP typu 'ask' (trh nakupuje)",
      "type": "object",
      "properties": {
        "price": {
          "type": "number",
          "description": "cena v měně currency"
        },
        "marketId": {
          "type": "string",
          "description": "Apl_Kod Instituce trhu, ze kterého pochází hodnoty (číselník)"
        },
        "datafeedId": {
          "type": [
            "string",
            "null"
          ],
          "description": "Apl_Kod Datafeedu, ze kterého pochází hodnoty (číselník)"
        }
      }
    },
    "lastTradePrice": {
      "description": "oceňovací cena CP typu 'trade'",
      "type": "object",
      "properties": {
        "price": {
          "type": "number",
          "description": "cena v měně currency"
        },
        "marketId": {
          "type": "string",
          "description": "Apl_Kod Instituce trhu, ze kterého pochází hodnoty (číselník)"
        },
        "datafeedId": {
          "type": [
            "string",
            "null"
          ],
          "description": "Apl_Kod Datafeedu, ze kterého pochází hodnoty (číselník)"
        }
      }
    },
    "lastTradeDate": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "title": "lastTradeDate",
      "description": "Datum a cas posledniho obchodu na burze Je-li lastTradePrice stanovena na zaklade ClosePrice, je datum bez casu. fond - datum stanoveni zaviraci ceny fondu (NAV)"
    },
    "prevClosePrice": {
      "type": [
        "number",
        "null"
      ],
      "title": "prevClosePrice",
      "description": "Zaviraci cena z predchoziho dne"
    },
    "prevCloseDate": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "title": "prevCloseDate",
      "description": "Datum predchozi close ceny"
    }
  }
}