{
  "title": "Card Limits",
  "$schema": "https://json-schema.org/draft-07/schema",
  "description": "Card Limits",
  "type": "object",
  "required": [
    "institutionId",
    "aPan",
    "pPan",
    "changeTime",
    "limits"
  ],
  "properties": {
    "X-Correlation-ID": {
      "type": [
        "string",
        "null"
      ],
      "title": "X-Correlation-ID",
      "description": "Correlation identifier"
    },
    "institutionId": {
      "type": "string",
      "title": "institutionId",
      "description": "Institution Id of bank which issue the card"
    },
    "aPan": {
      "type": "string",
      "title": "cardNumber",
      "description": "Card number on card which status have changed. CardNumber and MbrNum together are unique key"
    },
    "pPan": {
      "type": "string",
      "title": "cardNumber",
      "description": "Card number on card which status have changed. CardNumber and MbrNum together are unique key"
    },
    "aCardExpiration": {
      "type": [
        "string",
        "null"
      ],
      "title": "cardExpiration",
      "minLength": 4,
      "maxLength": 4,
      "description": "Card expiration - MMYY format"
    },
    "pCardExpiration": {
      "type": [
        "string",
        "null"
      ],
      "title": "cardExpiration",
      "minLength": 4,
      "maxLength": 4,
      "description": "Card expiration - MMYY format"
    },
    "changeTime": {
      "title": "changeTime",
      "description": "Message generation timestamp",
      "type": "string",
      "format": "date-time"
    },
    "limits": {
      "type": "array",
      "title": "limits",
      "description": "Array of limit objects",
      "items": {
        "$ref": "#/definitions/limit"
      }
    },
    "source": {
      "type": [
        "string",
        "null"
      ],
      "title": "source",
      "description": "Source of the Indicates system in which change was performed"
    }
  },
  "definitions": {
    "limit": {
      "type": "object",
      "required": [
        "limitType",
        "oldLimitValue",
        "newLimitValue"
      ],
      "properties": {
        "limitType": {
          "type": "string",
          "title": "limitType"
        },
        "oldLimitValue": {
          "type": "string",
          "title": "oldLimitValue."
        },
        "newLimitValue": {
          "type": "string",
          "title": "newLimitValue."
        }
      }
    }
  }
}