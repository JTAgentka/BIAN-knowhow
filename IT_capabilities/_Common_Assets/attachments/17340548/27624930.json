{
  "title": "StandingOrder",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "StandingOrder",
  "type": "object",
  "required": [
    "ptsId",
    "businessChangeDate",
    "type",
    "isInternal",
    "requestedExecutionDate",
    "instructedAmount",
    "interval",
    "mode",
    "modeDue",
    "businessState",
    "isAuthorized",
    "executionCount",
    "debtor",
    "creditor"
  ],
  "properties": {
    "X-Correlation-ID": {
      "type": [
        "string",
        "null"
      ],
      "title": "X-Correlation-ID",
      "description": "Correlation identifier"
    },
    "ptsId": {
      "type": "integer",
      "title": "ptsId",
      "description": "ID trvalého příkazu v PTS"
    },
    "businessChangeDate": {
      "type": "string",
      "format": "date-time",
      "title": "businessChangeDate",
      "description": "datum business změny stavu datové entity"
    },
    "identification": {
      "type": "object",
      "title": "identification",
      "description": "identification",
      "properties": {
        "instructionIdentification": {
          "type": "string",
          "title": "instructionIdentification",
          "description": "ID trvalého příkazu zaslané do PTS"
        },
        "operationId": {
          "type": "string",
          "title": "operationId",
          "description": "operation ID zasílané z externího systému"
        }
      }
    },
    "alias": {
      "type": [
        "string",
        "null"
      ],
      "title": "alias",
      "description": "uživatelský název příkazu"
    },
    "type": {
      "type": "string",
      "title": "type",
      "description": "typ trvalého příkazu",
      "enum": [
        "CZ",
        "FIN",
        "SEPA"
      ]
    },
    "isInternal": {
      "type": "boolean",
      "title": "isInternal",
      "description": "příznak, že je interní SO"
    },
    "requestedExecutionDate": {
      "type": "string",
      "format": "date",
      "title": "requestedExecutionDate",
      "description": "datum začátku"
    },
    "lastExecutionDate": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "title": "lastExecutionDate",
      "description": "datum konce"
    },
    "instructedAmount": {
      "type": "object",
      "title": "instructedAmount",
      "description": "instructedAmount",
      "required": [
        "value",
        "currency"
      ],
      "properties": {
        "value": {
          "type": "number",
          "title": "value",
          "description": "částka platby"
        },
        "currency": {
          "type": "string",
          "title": "currency",
          "description": "měna platby"
        }
      }
    },
    "lastPaymentDate": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "title": "lastPaymentDate",
      "description": "datum, kdy byl SO posledně spuštěn a vygeneroval platbu"
    },
    "intervalDue": {
      "type": [
        "integer",
        "null"
      ],
      "title": "intervalDue",
      "description": "den v intervalu, kdy má být příkaz spuštěn (WEEKLY interval 1-7, MONTHLY interval 1-31)"
    },
    "interval": {
      "type": "string",
      "title": "interval",
      "description": "interval opakování, týdně, měsíčně, ...",
      "enum": [
        "DAILY",
        "WEEKLY",
        "MONTHLY",
        "BI_MONTHLY",
        "QUARTERLY",
        "ONCE_PER_FOUR_MONTHS",
        "HALFYEARLY",
        "YEARLY"
      ]
    },
    "mode": {
      "type": "string",
      "title": "mode",
      "description": "definuje, kdy nebo jak bude trvalý příkaz ukončen",
      "enum": [
        "UNTIL_DATE",
        "UNTIL_CANCELLATION"
      ]
    },
    "nextDueDate": {
      "type": [
        "string",
        "null"
      ],
      "format": "date",
      "title": "nextDueDate",
      "description": "příští datum platby"
    },
    "modeDue": {
      "type": "string",
      "title": "modeDue",
      "description": "prováděcí pravidlo",
      "enum": [
        "DUE_DAY",
        "DUE_OR_BEFORE_DAY",
        "DUE_OR_NEXT_DAY",
        "LAST_DAY_OF_INTERVAL"
      ]
    },
    "businessState": {
      "type": "string",
      "title": "businessState",
      "description": "business status trvalého příkazu"
    },
    "exceptions": {
      "type": "object",
      "title": "exceptions",
      "description": "exceptions",
      "properties": {
        "stoppages": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string",
            "enum": [
              "JANUARY",
              "FEBRUARY",
              "MARCH",
              "APRIL",
              "MAY",
              "JUNE",
              "JULY",
              "AUGUST",
              "SEPTEMBER",
              "OCTOBER",
              "NOVEMBER",
              "DECEMBER"
            ]
          },
          "title": "stoppages",
          "description": "vynechat měsíce"
        },
        "break": {
          "type": "object",
          "title": "break",
          "description": "break",
          "required": [
            "breakFrom",
            "breakTo"
          ],
          "properties": {
            "breakFrom": {
              "type": "string",
              "format": "date",
              "title": "breakFrom",
              "description": "přerušení od"
            },
            "breakTo": {
              "type": "string",
              "format": "date",
              "title": "breakTo",
              "description": "přerušení do"
            }
          }
        }
      }
    },
    "endToEndIdentification": {
      "type": [
        "string",
        "null"
      ],
      "title": "endToEndIdentification",
      "description": "end to end reference"
    },
    "chargeBearer": {
      "type": [
        "string",
        "null"
      ],
      "title": "chargeBearer",
      "description": "kód poplatku",
      "enum": [
        "CRED",
        "DEBT",
        "SHAR",
        "SLEV"
      ]
    },
    "isAuthorized": {
      "type": "boolean",
      "title": "isAuthorized",
      "description": "příznak, že byl SO autorizovaný"
    },
    "executionCount": {
      "type": "integer",
      "title": "executionCount",
      "description": "počet provedení trvalého příkazu"
    },
    "symbols": {
      "type": "object",
      "title": "symbols",
      "description": "symbols",
      "properties": {
        "variableSymbol": {
          "type": [
            "string",
            "null"
          ],
          "title": "variableSymbol",
          "description": "variabilní symbol"
        },
        "constantSymbol": {
          "type": [
            "string",
            "null"
          ],
          "title": "constantSymbol",
          "description": "konstatní symbol"
        },
        "specificSymbol": {
          "type": [
            "string",
            "null"
          ],
          "title": "specificSymbol",
          "description": "specifický symbol"
        }
      }
    },
    "debtor": {
      "type": "object",
      "title": "debtor",
      "description": "odesílatel",
      "required": [
        "account",
        "bank",
        "name"
      ],
      "properties": {
        "account": {
          "type": "object",
          "title": "account",
          "description": "account",
          "required": [
            "other",
            "bban",
            "iban",
            "currency"
          ],
          "properties": {
            "other": {
              "type": "object",
              "title": "other",
              "description": "other",
              "required": [
                "identification"
              ],
              "properties": {
                "identification": {
                  "type": "string",
                  "title": "identification",
                  "description": "QST ID odesílatele"
                }
              }
            },
            "bban": {
              "type": "string",
              "title": "bban",
              "description": "bban odesílatele"
            },
            "iban": {
              "type": "string",
              "title": "iban",
              "description": "iban odesílatele"
            },
            "currency": {
              "type": "string",
              "title": "currency",
              "description": "měna účtu odesílatele"
            }
          }
        },
        "bank": {
          "type": "object",
          "title": "bank",
          "description": "bank",
          "required": [
            "bic",
            "bankcode"
          ],
          "properties": {
            "bic": {
              "type": "string",
              "title": "bic",
              "description": "BIC banky odesílatele"
            },
            "bankcode": {
              "type": "string",
              "title": "bankcode",
              "description": "kód banky odesílatele"
            },
            "name": {
              "type": "string",
              "title": "name",
              "description": "název banky odesílatele"
            },
            "postalAddress": {
              "type": "object",
              "title": "postalAddress",
              "description": "postalAddress",
              "properties": {
                "addressLine": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "title": "addressLine",
                  "description": "adresa banky odesílatele"
                },
                "country": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "title": "country",
                  "description": "kód země banky odesílatele"
                }
              }
            }
          }
        },
        "name": {
          "type": "string",
          "title": "name",
          "description": "jméno odesílatele"
        },
        "postalAddress": {
          "type": "object",
          "title": "postalAddress",
          "description": "postalAddress",
          "properties": {
            "addressLine": {
              "type": [
                "string",
                "null"
              ],
              "title": "addressLine",
              "description": "adresa odesílatele"
            },
            "country": {
              "type": [
                "string",
                "null"
              ],
              "title": "country",
              "description": "kód země odesílatele"
