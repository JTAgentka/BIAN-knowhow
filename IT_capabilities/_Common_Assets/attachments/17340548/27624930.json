        },
        "name": {
          "type": "string",
          "title": "name",
          "description": "jméno příjemce"
        },
        "postalAddress": {
          "type": "object",
          "title": "postalAddress",
          "description": "postalAddress",
          "properties": {
            "addressLine": {
              "type": [
                "string",
                "null"
              ],
              "title": "addressLine",
              "description": "adresa příjemce"
            },
            "country": {
              "type": [
                "string",
                "null"
              ],
              "title": "country",
              "description": "kód země příjemce"
            }
          }
        },
        "note": {
          "type": [
            "string",
            "null"
          ],
          "title": "note",
          "description": "poznámka pro příjemce"
