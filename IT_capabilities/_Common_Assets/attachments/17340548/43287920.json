  "type": "object",
  "required": [
    "confirmationId",
    "owner",
    "confirmationTypeId",
    "documentLanguage",
    "generatedDate",
    "insertedDate",
    "businessChangeDate",
    "stateId",
    "summedTrades"
  ],
  "properties": {
    "X-Correlation-ID": {
      "description": "Korelační identifikátor, povinné pro INIT a RECON",
      "type": [
        "string",
        "null"
      ]
    },
    "confirmationId": {
      "type": "integer",
      "description": "Id dokumentu"
    },
    "owner": {
      "type": "object",
      "description": "Detaily vlastníka",
      "$ref": "#/definitions/person"
    },
    "portfolioIds": {
      "type": "object",
      "description": "Identifikátory portfolia",
      "$ref": "#/definitions/portfolioIds"
    },
    "confirmationTypeId": {
      "description": "Hodnoty odkazující na typ konfirmace(bis10, ocp01,ocp03, ocp04, ocp06, ocp08, ocp10, ocp12, ocp13, ocp14, ocp24, ocp37, ocp39, ocp40, ocp41, ocp43, ocp44, ocp45, ocp46)",
