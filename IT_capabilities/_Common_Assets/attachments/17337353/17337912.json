					},
					{
						"name": "channel",
						"in": "query",
						"required": false,
						"schema": {
							"type": "array",
							"items": {
								"type": "string",
								"nullable": true,
								"enum": [
									"CZ",
									"SK"
								]
							}
						}
					}
				],
				"responses": {
