{
	"openapi": "3.0.0",
	"info": {
		"title": "FxRates Reconciliation",
		"description": "The service is used to reconcile values of exchange rates according to specified input parameters.",
		"version": "v1"
	},
	"paths": {
		"/fxRatesInstant/": {
			"get": {
				"description": "The method is used to reconcile values of Instant exchange rates according to specified input parameters.",
				"parameters": [{
						"name": "X-Corellation-Id",
						"in": "header",
						"required": true,
						"schema": {
							"type": "string",
							"description": "Unique request ID. If the same request is sent again, this ID does not change"
						}
					},
					{
						"name": "traceparent",
						"in": "header",
						"required": true,
						"schema": {
							"type": "string",
							"description": "Parameter for central logging, corresponding to the W3C Trace Context standard."
						}
					},
					{
						"name": "validityStartDate",
						"in": "query",
						"required": false,
						"schema": {
							"type": "string",
							"format": "date-time"
						}
					},
					{
						"name": "validityEndDate",
						"in": "query",
						"required": false,
						"schema": {
							"type": "string",
							"format": "date-time"
						}
					},
					{
						"name": "businessChangeDateStart",
						"in": "query",
						"required": false,
						"schema": {
							"type": "string",
							"format": "date-time"
						}
					},
					{
						"name": "businessChangeDateEnd",
						"in": "query",
						"required": false,
						"schema": {
							"type": "string",
							"format": "date-time"
						}
					},
					{
						"name": "fxRateListId",
						"in": "query",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "channel",
						"in": "query",
						"required": false,
						"schema": {
							"type": "array",
							"items": {
								"type": "string",
								"nullable": true,
								"enum": [
									"CZ",
									"SK"
								]
							}
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/asyncResponse"
								}
							}
						}
					},
					"204": {
						"description": "No Content"
					},
					"500": {
						"description": "Internal server error"
					},
					"503": {
						"description": "Service Unavailable"
					}
				}
			}
		},
