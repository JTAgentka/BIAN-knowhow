{
   "title":"DDM",
   "$schema":"https://json-schema.org/draft/2020-12/schema",
   "description":"DDM",
   "type":"object",
   "properties":{
      "ptsId":{
         "type":"integer",
         "title":"ptsId",
         "description":"id platby PTS"
      },
      "identification":{
         "type":"object",
         "title":"identification",
         "description":"identification",
         "properties":{
            "instructionIdentification":{
               "type":"string",
               "title":"instructionIdentification",
               "description":"id DDM zaslané do PTS"
            },
            "operationId":{
               "type":"string",
               "title":"operationId",
               "description":"operation id zaslané z externího systému"
            }
         }
      },
      "alias":{
         "type":"string",
         "title":"alias",
         "description":"uživatelský název DDM"
      },
      "isRepeat":{
         "type":"boolean",
         "title":"isRepeat",
         "description":"příznak opakování"
      },
      "requestedExecutionDate":{
         "type":"string",
         "title":"requestedExecutionDate",
         "description":"datum odkdy je DDM platné"
      },
      "lastExecutionDate":{
         "type":"string",
         "title":"lastExecutionDate",
         "description":"datum konce + i datum ukončení od uživatele"
      },
      "scheme":{
         "type":"string",
         "title":"scheme",
         "description":"schéma"
      },
      "mandateId":{
         "type":"string",
         "title":"mandateId",
         "description":"id mandátu"
      },
      "cid":{
         "type":"string",
         "title":"cid",
         "description":"cid"
      },
      "amountSingleLimit":{
         "type":"number",
         "title":"amountSingleLimit",
         "description":"limit pro jednotlivou částku"
      },
      "amountSumLimit":{
         "type":"number",
         "title":"amountSumLimit",
         "description":"limit pro součet částek"
      },
      "maxIterations":{
         "type":"integer",
         "title":"maxIterations",
         "description":"limit pro maximální počet opakování"
      },
      "referredDocument":{
         "type":"string",
         "title":"referredDocument",
         "description":"refereční dokument"
      },
      "dateOfSignature":{
         "type":"string",
         "title":"dateOfSignature",
         "description":"datum podpisu referenčního dokumentu"
      },
      "debtor":{
         "type":"object",
         "title":"debtor",
         "description":"debtor",
         "properties":{
            "account":{
               "type":"object",
               "title":"account",
               "description":"account",
               "properties":{
                  "other":{
                     "type":"object",
                     "title":"other",
                     "description":"other",
                     "properties":{
                        "identification":{
                           "type":"string",
                           "title":"identification",
                           "description":"QSTID odesílatele"
                        }
                     }
                  }
               },
               "iban":{
                  "type":"string",
                  "title":"iban",
                  "description":"iban odesílatele"
               },
               "currency":{
                  "type":"string",
                  "title":"currency",
                  "description":"měna účtu odesílatele"
               }
            },
            "bank":{
               "type":"object",
               "title":"bank",
               "description":"bank",
               "properties":{
                  "bic":{
                     "type":"string",
                     "title":"bic",
                     "description":"BIC banky odesílatele"
                  }
               }
            },
            "name":{
               "type":"string",
               "title":"name",
               "description":"jméno odesílatele"
            }
         }
      },
      "creditor":{
         "type":"object",
         "title":"creditor",
         "description":"creditor",
         "properties":{
            "account":{
               "type":"object",
               "title":"account",
               "description":"account",
               "properties":{
                  "other":{
                     "type":"object",
                     "title":"other",
                     "description":"other",
                     "properties":{
                        "identification":{
                           "type":"string",
                           "title":"identification",
                           "description":"QSTID příjemce"
                        }
                     }
                  },
                  "iban":{
                     "type":"string",
                     "title":"iban",
                     "description":"iban příjemce"
                  },
                  "currency":{
                     "type":"string",
                     "title":"currency",
                     "description":"měna účtu příjemce"
                  }
               }
            },
            "bank":{
               "type":"object",
               "title":"bank",
               "description":"bank",
               "properties":{
                  "bic":{
                     "type":"string",
                     "title":"bic",
                     "description":"BIC banky příjemce"
                  }
               }
            },
            "name":{
               "type":"string",
               "title":"name",
               "description":"jméno příjemce"
            }
         }
      },
      "note":{
         "type":"string",
         "title":"note",
         "description":"poznámka"
      },
      "state":{
         "type":"string",
         "title":"state",
         "description":"stav"
      },
      "channel":{
         "type":"string",
         "title":"channel",
         "description":"komunikační kanál"
      },
      "lastPaymentDate":{
         "type":"string",
         "title":"lastPaymentDate",
         "description":"datum poslední vygenerované platby"
      },
      "amountLimitUsed":{
         "type":"integer",
         "title":"amountLimitUsed",
         "description":"součet částek všech vygenerovaných inkas"
      },
      "iterationsCount":{
         "type":"integer",
         "title":"iterationsCount",
         "description":"počet vygenerovaných plateb "
      }
   },
   "additionalProperties":false
}